{
    "description": "My Framework based from PSR",
    "name": "faridjan/my_framework",
    "minimum-stability": "stable",
    "license": "proprietary",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "ext-PDO": "^7.1",
        "ext-json": "*",
        "aura/router": "^3.1",
        "doctrine/migrations": "2.2.*",
        "laminas/laminas-config-aggregator": "^1.4",
        "laminas/laminas-diactoros": "^2.4",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stratigility": "^3.3",
        "monolog/monolog": "^2.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "roave/psr-container-doctrine": "^2.2",
        "robmorgan/phinx": "^0.12.4",
        "symfony/console": "^5.1"
    },
    "require-dev": {
        "filp/whoops": "^2.9",
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Farid\\": "src/",
            "Farid\\Bin\\": "bin/",
            "Farid\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Farid",
            "email": "fred.andersen7@gmail.com"
        }
    ]
}
