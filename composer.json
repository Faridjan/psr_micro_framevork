{
  "description": "My Framework based from PSR",
  "name": "faridjan/my_framework",
  "minimum-stability": "stable",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "aura/router": "^3.1",
    "laminas/laminas-diactoros": "^2.4",
    "laminas/laminas-httphandlerrunner": "^1.2",
    "laminas/laminas-stratigility": "^3.3",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Farid\\": "src/",
      "Farid\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Farid",
      "email": "fred.andersen7@gmail.com"
    }
  ]
}
